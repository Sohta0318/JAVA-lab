package children;

import parent.Aniamal;

public class fish extends Aniamal {
	private String feature;
	private String habitat;
	private boolean gills;
	

	public fish(int height, int weight, String animal_type, String blood_type, String feature, String habitat, Boolean gills) {
		super(height, weight, animal_type, blood_type);
		// TODO Auto-generated constructor stub
		this.habitat = "water";
		this.gills = true;
	}

	public String getFeature() {
		return feature;
	}

	public String getHabitat() {
		return habitat;
	}

	public boolean isGills() {
		return gills;
	}

	

}




